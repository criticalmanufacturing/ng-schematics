{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Update an application with HTML defaults.",
      "factory": "./ng-generator-html/ng-add/index",
      "schema": "./ng-generator-html/ng-add/schema.json",
      "save": "devDependencies",
      "private": true,
      "hidden": true
    },
    "wizard": {
      "description": "Generate a new wizard.",
      "factory": "./ng-generator-html/wizard",
      "schema": "./ng-generator-html/wizard/schema.json"
    },
    "wizard-create-edit": {
      "description": "Generates a new create edit wizard.",
      "factory": "./ng-generator-html/wizard-create-edit",
      "schema": "./ng-generator-html/wizard-create-edit/schema.json"
    }
  }
}